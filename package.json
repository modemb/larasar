  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "fca": "quasar dev -H 192.168.2.11 -m capacitor -T android",
    "fco": "quasar dev -H 192.168.2.11 -m android",
    "bca": "quasar build -m capacitor -T android",
    "php": "php -S 192.168.2.11:8000 -t public",
